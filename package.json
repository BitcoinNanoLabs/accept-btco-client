{
  "version": "2.0.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": ["dist", "src"],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsc -w --noEmit && parcel src/index.html --global acceptNano --no-cache",
    "build": "parcel build src/index.ts --global acceptNano --no-cache --out-file index.js",
    "build:example": "cp src/index.html example/index.html && replace-in-file ./index.ts https://unpkg.com/@accept-nano/client example/index.html",
    "test": "jest",
    "coveralls": "yarn test --coverage && cat ./coverage/lcov.info | coveralls",
    "prettier": "prettier --write ./src"
  },
  "peerDependencies": {},
  "prettier": "@putdotio/prettier-config",
  "name": "@accept-nano/client",
  "author": "Altay Aydemir",
  "devDependencies": {
    "@putdotio/prettier-config": "^1.0.0",
    "@types/big.js": "^6.0.0",
    "@types/jest": "^26.0.14",
    "@types/qrcode": "^1.3.5",
    "axios-mock-adapter": "^1.18.2",
    "coveralls": "^3.1.0",
    "jest": "^26.5.3",
    "jest-canvas-mock": "^2.3.0",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.1.2",
    "replace-in-file": "^6.1.0",
    "ts-jest": "^26.4.1",
    "typescript": "^4.0.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@byungi/event-emitter": "^1.2.2",
    "axios": "^0.20.0",
    "big.js": "^6.0.1",
    "qrcode": "^1.4.4",
    "redom": "^3.27.1",
    "spin.js": "^4.1.0",
    "xstate": "^4.13.0"
  }
}
